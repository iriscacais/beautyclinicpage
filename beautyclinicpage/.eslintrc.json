{
    "extends": ["airbnb", "prettier"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "babelOptions": {
        "presets": ["@babel/preset-react"]
     }
   
  },
    "plugins": ["react", "prettier"],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [".js", "jsx"]
        }
      ],
      "max-len": ["error", 2000],
      "react/prop-types": 0
    },
    "env": {
      "browser": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".jsx", ".ts", ".tsx"],
          "paths": ["src"]
        }
      }
    }
  }